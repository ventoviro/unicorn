  "homepage": "https://github.com/ventoviro/unicorn#readme",
  "dependencies": {
    "alpinejs": "^2.8.2",
    "awesome-bootstrap-checkbox": "^1.0.1",
    "axios": "^0.21.1",
    "gulp": "^4.0.2",
    "systemjs": "^6.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
