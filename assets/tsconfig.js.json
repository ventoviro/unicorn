{
    "include": [
        "./src/types/index.d.ts"
    ],
    "exclude": [
        "node_modules/**/*"
    ],
    "compilerOptions": {
        "allowJs": true,
        "module": "system",
        "moduleResolution": "node",
        "target": "es2020",
        "skipLibCheck": true,
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "noImplicitThis": true,
        "strict": true,
        "useDefineForClassFields": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    }
}
